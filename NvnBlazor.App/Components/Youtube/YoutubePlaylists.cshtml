@inherits YoutubePlaylistsModel

<div class="d-flex flex-fill ">
    @foreach (var s in YoutubeViewModelsList)
    {
        <a href="@s.VideoLink" class="btn btn-info btn-block" role="button" target="_blank" >@s.Title</a>
     
    }

</div>



@functions
{ protected override async Task OnInitAsync()
{
    await GetYoutubePlaylists();
}
}
